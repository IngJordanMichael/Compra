
<table class="table table-hover table-dark">
  <thead>
    <tr  class="bg-danger">
      <th scope="">#</th>
      <th scope="col">Client</th>
      <th scope="col">Product</th>
      <th scope="col">Total to pay</th>
      <th scope="col">Purchase date</th>
    </tr>
  </thead>
  <tbody>
      {{#each sale}}
    <tr >
      <th scope="row">1</th>
      <td>{{cliente}}</td>
      <td>{{product}}</td>
      <td>${{Monto}}</td>
      <td>{{created}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
